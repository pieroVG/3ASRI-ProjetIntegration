<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="motoman_hc10" params="prefix">
    <xacro:include filename="$(find motoman_resources)/urdf/common_materials.xacro"/>
    
    
    <!-- link list -->
    <link name="world"/>
    
    <link name="${prefix}base_link">
      <visual>
        <geometry>
          <mesh filename="package://motoman_hc10_support/meshes/hc10/visual/base_link.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_hc10_support/meshes/hc10/collision/base_link.stl"/>
        </geometry>
      </collision>
      <inertial>
      	 <origin xyz="0 0 0" rpy="0 0 0"/>
      	 <mass value="0.1"/>
      	 <inertia ixx="0.3" ixy="0.03" ixz="0.03" iyy="0.0" iyz="0.0" izz="0.0"/>
      </inertial>
    </link>
    
    <link name="${prefix}link_1_s">
      <visual>
        <geometry>
          <mesh filename="package://motoman_hc10_support/meshes/hc10/visual/link_1_s.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_hc10_support/meshes/hc10/collision/link_1_s.stl"/>
        </geometry>
      </collision>
      <inertial>
      	 <origin xyz="0 0 0" rpy="0 0 0"/>
      	 <mass value="0.1"/>
      	 <inertia ixx="0.3" ixy="0.03" ixz="0.03" iyy="0.0" iyz="0.0" izz="0.0"/>
      </inertial>
    </link>
    
    <link name="${prefix}link_2_l">
      <visual>
        <geometry>
          <mesh filename="package://motoman_hc10_support/meshes/hc10/visual/link_2_l.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_hc10_support/meshes/hc10/collision/link_2_l.stl"/>
        </geometry>
      </collision>
      <inertial>
      	 <origin xyz="0 0 0" rpy="0 0 0"/>
      	 <mass value="0.1"/>
      	 <inertia ixx="0.3" ixy="0.03" ixz="0.03" iyy="0.0" iyz="0.0" izz="0.0"/>
      </inertial>
    </link>
    
    <link name="${prefix}link_3_u">
      <visual>
        <geometry>
          <mesh filename="package://motoman_hc10_support/meshes/hc10/visual/link_3_u.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_hc10_support/meshes/hc10/collision/link_3_u.stl"/>
        </geometry>
      </collision>
      <inertial>
      	 <origin xyz="0 0 0" rpy="0 0 0"/>
      	 <mass value="0.1"/>
      	 <inertia ixx="0.3" ixy="0.03" ixz="0.03" iyy="0.0" iyz="0.0" izz="0.0"/>
      </inertial>
    </link>
    
    <link name="${prefix}link_4_r">
      <visual>
        <geometry>
          <mesh filename="package://motoman_hc10_support/meshes/hc10/visual/link_4_r.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_hc10_support/meshes/hc10/collision/link_4_r.stl"/>
        </geometry>
      </collision>
      <inertial>
      	 <origin xyz="0 0 0" rpy="0 0 0"/>
      	 <mass value="0.1"/>
      	 <inertia ixx="0.3" ixy="0.03" ixz="0.03" iyy="0.0" iyz="0.0" izz="0.0"/>
      </inertial>
    </link>
    
    <link name="${prefix}link_5_b">
      <visual>
        <geometry>
          <mesh filename="package://motoman_hc10_support/meshes/hc10/visual/link_5_b.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_hc10_support/meshes/hc10/collision/link_5_b.stl"/>
        </geometry>
      </collision>
      <inertial>
      	 <origin xyz="0 0 0" rpy="0 0 0"/>
      	 <mass value="0.1"/>
      	 <inertia ixx="0.3" ixy="0.03" ixz="0.03" iyy="0.0" iyz="0.0" izz="0.0"/>
      </inertial>
    </link>
    
    <link name="${prefix}link_6_t">
      <visual>
        <geometry>
          <mesh filename="package://motoman_hc10_support/meshes/hc10/visual/link_6_t.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_hc10_support/meshes/hc10/collision/link_6_t.stl"/>
        </geometry>
      </collision>
      <inertial>
      	 <origin xyz="0 0 0" rpy="0 0 0"/>
      	 <mass value="0.1"/>
      	 <inertia ixx="0.3" ixy="0.03" ixz="0.03" iyy="0.0" iyz="0.0" izz="0.0"/>
      </inertial>
    </link>
    
    <link name="${prefix}tool0"/>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="world_prefix" type="fixed">
      <parent link="world"/>
      <child link="${prefix}base_link"/>
    </joint>
    
    <joint name="${prefix}joint_1_s" type="revolute">
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link_1_s"/>
      <origin xyz="0 0 0.275" rpy="0 0 0" />
      <axis xyz="0 0 1" />
      <limit lower="${radians(-180)}" upper="${radians(180)}" effort="368.48" velocity="${radians(130)}"/>
      <dynamics damping="1"/>
    </joint>
    
    <joint name="${prefix}joint_2_l" type="revolute">
      <parent link="${prefix}link_1_s"/>
      <child link="${prefix}link_2_l"/>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <axis xyz="0 1 0" />
      <limit lower="${radians(-180)}" upper="${radians(180)}" effort="368.48" velocity="${radians(130)}"/>
      <dynamics damping="1"/>
    </joint>
    
    <joint name="${prefix}joint_3_u" type="revolute">
      <parent link="${prefix}link_2_l"/>
      <child link="${prefix}link_3_u"/>
      <origin xyz="0 0 0.700" rpy="0 0 0" />
      <axis xyz="0 -1 0" />
      <limit lower="${radians(-5)}" upper="${radians(355)}" effort="141.12" velocity="${radians(180)}"/>
      <dynamics damping="1"/>
    </joint>
    
    <joint name="${prefix}joint_4_r" type="revolute">
      <parent link="${prefix}link_3_u"/>
      <child link="${prefix}link_4_r"/>
      <origin xyz="0 0 -0.500" rpy="0 0 0" />
      <axis xyz="0 0 1" />
      <limit lower="${radians(-180)}" upper="${radians(180)}" effort="39.20" velocity="${radians(180)}"/>
      <dynamics damping="1"/>
    </joint>
    
    <joint name="${prefix}joint_5_b" type="revolute">
      <parent link="${prefix}link_4_r"/>
      <child link="${prefix}link_5_b"/>
      <origin xyz="0 0.162 0" rpy="0 0 0" />
      <axis xyz="0 -1 0" />
      <limit lower="${radians(-180)}" upper="${radians(180)}" effort="31.36" velocity="${radians(250)}"/>
      <dynamics damping="1"/>
    </joint>
    
    <joint name="${prefix}joint_6_t" type="revolute">
      <parent link="${prefix}link_5_b"/>
      <child link="${prefix}link_6_t"/>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <axis xyz="0 0 1" />
      <limit lower="${radians(-180)}" upper="${radians(180)}" effort="31.36" velocity="${radians(250)}"/>
      <dynamics damping="1"/>
    </joint>
    
    <joint name="${prefix}joint_6_t-tool0" type="fixed">
      <origin xyz="0 0 -0.130" rpy="${pi} 0 0"/>
      <parent link="${prefix}link_6_t"/>
      <child link="${prefix}tool0"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0.275" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>
    
    <xacro:macro name="control" params="joint">
        <transmission name="${joint}_transmission">
            <type>transmission_interface/SimpleTransmission</type>
            <!-- The EffortJointInterface works best with Gazebo using torques to actuate the joints -->
            <joint name="${joint}">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            </joint>
            <actuator name="${joint}_motor">
                <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
                <mechanicalReduction>1</mechanicalReduction>
            </actuator>
        </transmission>
    </xacro:macro>
    
    <gazebo>
      <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
        <robotNamespace>/${prefix}</robotNamespace>
        <legacyModeNS>true</legacyModeNS>
      </plugin>
    </gazebo>
    
	<!-- Transmissions: PositionJointInterface simple -->
	<transmission name="${prefix}trans_joint_1_s">
	  <type>transmission_interface/SimpleTransmission</type>
	  <joint name="${prefix}joint_1_s">
		<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
	  </joint>
	  <actuator name="${prefix}motor_1_s">
		<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
		<mechanicalReduction>1</mechanicalReduction>
	  </actuator>
	</transmission>

	<transmission name="${prefix}trans_joint_2_l">
	  <type>transmission_interface/SimpleTransmission</type>
	  <joint name="${prefix}joint_2_l">
		<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
	  </joint>
	  <actuator name="${prefix}motor_2_l">
		<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
		<mechanicalReduction>1</mechanicalReduction>
	  </actuator>
	</transmission>

	<transmission name="${prefix}trans_joint_3_u">
	  <type>transmission_interface/SimpleTransmission</type>
	  <joint name="${prefix}joint_3_u">
		<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
	  </joint>
	  <actuator name="${prefix}motor_3_u">
		<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
		<mechanicalReduction>1</mechanicalReduction>
	  </actuator>
	</transmission>

	<transmission name="${prefix}trans_joint_4_r">
	  <type>transmission_interface/SimpleTransmission</type>
	  <joint name="${prefix}joint_4_r">
		<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
	  </joint>
	  <actuator name="${prefix}motor_4_r">
		<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
		<mechanicalReduction>1</mechanicalReduction>
	  </actuator>
	</transmission>

	<transmission name="${prefix}trans_joint_5_b">
	  <type>transmission_interface/SimpleTransmission</type>
	  <joint name="${prefix}joint_5_b">
		<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
	  </joint>
	  <actuator name="${prefix}motor_5_b">
		<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
		<mechanicalReduction>1</mechanicalReduction>
	  </actuator>
	</transmission>

	<transmission name="${prefix}trans_joint_6_t">
	  <type>transmission_interface/SimpleTransmission</type>
	  <joint name="${prefix}joint_6_t">
		<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
	  </joint>
	  <actuator name="${prefix}motor_6_t">
		<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
		<mechanicalReduction>1</mechanicalReduction>
	  </actuator>
	</transmission>


  </xacro:macro>
</robot>
